
<div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
    <div class="container">
        <div class="row mt-3">
            <div class="col tabParam">
                Email
            </div>
            <div class="col">
                @Email
            </div>
        </div>
        <div class="row mt-3">
            <div class="col tabParam">
                Phone
            </div>
            <div class="col">
                @Phone
            </div>
        </div>
    </div>
</div>

@code {
    [Parameter]
    public string Email { get; set; }
    [Parameter]
    public string Phone { get; set; }
}
